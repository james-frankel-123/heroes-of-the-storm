# Environment Variables Example
# Copy this file to .env.local and fill in your values

# Heroes Profile API (Required)
HEROES_PROFILE_API_KEY=your_heroes_profile_api_key_here

# OpenAI API (Required for AI commentary features)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Model Configuration
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=800

# Feature Flags (optional)
# NEXT_PUBLIC_ENABLE_COMPARISON_MODE=false
# NEXT_PUBLIC_ENABLE_ACHIEVEMENTS=false
# NEXT_PUBLIC_ENABLE_AI_COMMENTARY=true

# Analytics (optional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
